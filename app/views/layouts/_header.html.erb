<header>

  <div class="main-header group">
    <div id="search-bar"></div>
    <div class="user_session">
      <% if !signed_in? %>
      <ul class="user_session_list">
        <li><%= link_to('Sign Up', new_user_url, class: 'sign_up_button') %></li>
        <li><%= link_to('Log In', new_session_url, class: 'log_in_button') %></li>
      </ul>
      <% else %>
        <%= link_to('Log Out', '/session', method: 'DELETE') %>
      <% end %>
    </div>
  </div>

</header>
